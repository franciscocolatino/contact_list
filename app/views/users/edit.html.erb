<div class="row">
    <div class="col-xs-12">
        <h1>Editar dados</h1>
    </div>    

    <div class="col-md-6 col-md-offset-3 col-xs-12">
        <%= form_for current_user do |f| %>
            <%= render 'layouts/error_messages', object: current_user %>
            <%= render 'form', f: f %>
        <%= f.submit 'Cadastrar', class: 'btn-primary' %>
        <% end %>
    </div>
</div>